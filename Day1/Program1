#include <stdio.h>

int main()
{
    printf("enter two numbers=");
    int a;
    int b;
    scanf("%d%d",&a,&b);
    int sum=a+b;
    int sub=(a>b)?a-b:b-a;
    int div=(a>b)?a/b:b/a;
    int mul=a*b;
    printf("\n sum = %d",sum);
    printf("\n Subraction = %d",sub);
    printf(" \n div = %d",div);
    printf("\n Multiplication = %d",mul);
    return 0;
}
